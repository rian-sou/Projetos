#include <iostream>
using namespace std;

float somaDeFloats(float a, float b);

int main()
{
	float a;
	float b;

	cin >> a >> b;

	float soma = somaDeFloats(a, b);

	cout << "a + b = " << soma;

	return 0;
}

float somaDeFloats(float a, float b)
{
	if (a == b)
		return (a + b) * 2;
	else
		return a + b;
}

