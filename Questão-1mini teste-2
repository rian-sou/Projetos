#include <iostream>
using namespace std;        

float somaDeFloats(float a, float b);

int main() {
float a = 30;
float b = 30;


cin >> a >> b;

float s = somaDeFloats(a,b);

cout << "a + b = " << s;

return 0;
}


float somaDeFloats(float a, float b) {
if (a == b)
  return (a+b)*2;
  else 
  return a + b;
}
