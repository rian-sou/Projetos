#include <iostream> 

using namespace std;

struct Ponto
{
    int p[2];
};

struct Ret
{
    int v1[2] = {2, 1};
    int v2[2] = {12, 11};
};

int main()
{
    Ret r1;
    Ponto p1;

    cout << "Agora por favor, digite os valores do ponto (x e y respectivamente)";
    cin >> p1.p[1] >> p1.p[2];

    if(p1.p[1] >= r1.v1[1] and p1.p[2] <= r1.v2[2])
    {
        cout << "O ponto está dentro do retangulo" << endl;
    }
    else
        cout << "O ponto nao está dentro do retangulo" << endl;

    return 0;
}
